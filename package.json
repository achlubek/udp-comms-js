{
  "name": "udp-comms-foundational",
  "author": "Adrian Chlubek",
  "version": "1.0.0",
  "main": "dist/main.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint \"{src,test}/**/*.ts\" --max-warnings=0",
    "lint:fix": "eslint --fix \"{src,test}/**/*.ts\"",
    "prettier": "prettier --check \"**/*.((j|t)s|json|yml|yaml)\" --ignore-path .gitignore",
    "prettier:fix": "prettier --write \"**/*.((j|t)s|json|yml|yaml)\" --ignore-path .gitignore",
    "style:fix": "yarn prettier:fix && yarn lint:fix",
    "test": "mocha --config .mocharc.json",
    "test:coverage": "nyc --reporter=html yarn mocha",
    "generate-reflection": "aero-di-generate --baseDir src",
    "build": "yarn generate-reflection && yarn rimraf dist && tsc -P tsconfig.build.json && resolve-tspaths --project tsconfig.build.json",
    "start:dev": "ts-node -r dotenv/config -r tsconfig-paths/register src/main.ts",
    "start:dist": "node dist/main.ts"
  },
  "dependencies": {
    "aero-cqe": "^1.0.2",
    "aero-di": "^1.1.1",
    "joi": "17.*",
    "tsconfig-paths": "4.*"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.*",
    "@types/chai": "4.*",
    "@types/mocha": "10.*",
    "@types/node": "18.*",
    "@types/sinon": "10.*",
    "@typescript-eslint/eslint-plugin": "5.*",
    "@typescript-eslint/parser": "5.*",
    "browserify": "^17.0.0",
    "chai": "4.*",
    "cross-env": "7.*",
    "dotenv": "^16.0.3",
    "eslint": "8.*",
    "eslint-config-prettier": "8.*",
    "eslint-plugin-import": "2.*",
    "eslint-plugin-prettier": "4.*",
    "mocha": "10.*",
    "nyc": "15.*",
    "prettier": "2.*",
    "resolve-tspaths": "^0.8.8",
    "rimraf": "^4.1.2",
    "sinon": "15.*",
    "ts-node": "10.*",
    "typescript": "4.*"
  }
}
